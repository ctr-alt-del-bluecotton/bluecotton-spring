<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.bluecotton.mapper.ChatMemberMapper">
    <insert id="insertChatMember" parameterType="ChatMemberVO">
        INSERT INTO
        TBL_CHAT_MEMBER
        VALUES (SEQ_CHAT_MEMBER.NEXTVAL, #{chatId}, #{memberId});
    </insert>

    <delete id="delete" parameterType="MyChatRoomCustomerVO">
        DELETE FROM TBL_MY_CHAT_ROOM_CUSTOMER
        WHERE MEMBER_ID = #{memberId} AND CHAT_ID = #{chatId}
    </delete>

</mapper>